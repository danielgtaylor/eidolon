{
  "name": "eidolon",
  "version": "1.5.3",
  "description": "Generate JSON or JSON Schema from Refract & MSON data structures",
  "main": "lib/eidolon.js",
  "scripts": {
    "build": "coffee -o lib -c src",
    "cover": "istanbul report text-summary lcov",
    "test": "coffeelint src/*.coffee test/*.coffee && mocha",
    "precoveralls": "npm run cover",
    "coveralls": "coveralls <coverage/lcov.info",
    "prepublish": "npm run build"
  },
  "keywords": [
    "refract",
    "mson",
    "api",
    "blueprint",
    "json",
    "schema"
  ],
  "author": "Daniel G. Taylor <danielgtaylor@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "chai": "^3.5.0",
    "coffee-coverage": "^1.0.1",
    "coffee-script": "^1.10.0",
    "coffeelint": "^1.14.2",
    "coveralls": "^2.11.6",
    "glob": "^6.0.4",
    "istanbul": "^0.4.2",
    "mocha": "^2.4.4",
    "sinon": "^1.17.3"
  },
  "dependencies": {
    "faker": "github:marak/faker.js#9bb2b7c"
  }
}
